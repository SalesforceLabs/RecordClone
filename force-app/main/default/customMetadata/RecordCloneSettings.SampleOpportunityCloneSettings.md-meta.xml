<CustomMetadata
  xmlns="http://soap.sforce.com/2006/04/metadata"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
>
    <label>Sample Opportunity Clone Settings</label>
    <protected>false</protected>
    <values>
        <field>ChildRecordNameType__c</field>
        <value
      xsi:type="xsd:string"
    >Cloned - [Child Record Name] - [YYYY/mm/dd HH:ii]</value>
    </values>
    <values>
        <field>ChildRelationshipNames__c</field>
        <value
      xsi:type="xsd:string"
    >OpportunityLineItems, OpportunityContactRoles</value>
    </values>
    <values>
        <field>ExcludedFieldNames__c</field>
        <value
      xsi:type="xsd:string"
    >IqScore, CampaignId, ExpectedRevenue, Description</value>
    </values>
    <values>
        <field>ObjectName__c</field>
        <value xsi:type="xsd:string">Opportunity</value>
    </values>
</CustomMetadata>
